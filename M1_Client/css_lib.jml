				var cssLib=this.cssLib;
				var page=this.page;
				return {
					"type":"key","id":"Button","pos":pos,w:w,h:80,"css":imgLib.btn_green_u,key:keyid,mode3x3:1,"anchor_h":1,"anchor_v":1,"ui_event":1,"color_a":255,button:1,down_scale:0.9,
					"size3x3":[28,0,28,0],"uv3x3":[28/1024,0,28/1024,0],
					state_up:{"css":imgLib.btn_green_u,mode3x3:1,w:w,h:80,"size3x3":[28,0,28,0],"uv3x3":[28/1024,0,28/1024,0],},
					state_down:{"css":imgLib.btn_green_d,mode3x3:1,w:w-8,h:80-8,"size3x3":[28,0,28,0],"uv3x3":[28/1024,0,28/1024,0],},
					state_gray:{"css":imgLib.btn_green_g,mode3x3:1,w:w,h:80,"size3x3":[28,0,28,0],"uv3x3":[28/1024,0,28/1024,0],},
					items:[
						cssLib.textFineBig.createCSS([0,0,0],text,w,80,1,1,1,1,bad?[255,0,0]:[255,255,255]),
					],
					display:1,fade:1,fade_size:1,fade_alpha:0,
					page:page,
					setText:this.setText
				};
			},
			create:function(box,pos,w,num,bad,key)
			{
				return box.appendNewChild(this.createCSS(pos,w,num,bad,key));
			},
			setText:function(text,bad)
			{
				var url;
				this.firstChild()._setText(text);
				if(bad)
				{
					this.setEnabled(0);
					this.firstChild().setColor(255,0,0,255);
				}
				else
				{
					this.setEnabled(1);
					this.firstChild().setColor(255,255,255,255);
				}
			}
