				var cssLib=this.cssLib;
				var page=this.page;
				var css;
				var url, pic;
				h=h?h:this.h;
				url=page.genPageURL(window.imgPath+"/icon/icon_res_"+res+"64_32.png");
				pic=imgLib.getImg("btn_green_u"),picW=w,picH=pic.h;
				
				css={
					type:"key",id:"BtnResGO",pos:pos,w:w,h:h,css:imgLib.btn_green_u,key:keyid,mode3x3:1,anchor_h:1,anchor_v:1,ui_event:1,color_a:255,button:1,down_scale:0.95,
					state_up:{css:imgLib.btn_green_u,mode3x3:1,w:w,h:h,},audio:this.page.audioObject.genFileURL("btn_click"),
					state_down:{css:imgLib.btn_green_d,mode3x3:1,w:w,h:h,},
					state_gray:{css:imgLib.btn_green_g,mode3x3:1,w:w,h:h,},
					items:[
						{id:"num",css:cssLib.textFineBig.createCSS([0,0,0],""+num,w-140,h,1,1,2,1,bad?[255,0,0]:[255,255,255])},
						{type:"icon",id:"ResIcon",pos:[Math.floor(w/2-70),0,0],w:64,h:64,tex:url,tex_u:0,tex_v:0,tex_w:1,tex_h:1,anchor_h:0,anchor_v:1}
					],
					display:1,fade:1,fade_size:1,fade_alpha:0,
					page:page,
					setResNum:this.setResNum
				};
				return css;
			},
			create:function(box,pos,w,res,num,bad,key,h)
			{
				return box.appendNewChild(this.createCSS(pos,w,res,num,bad,key,h));
			},
			setResNum:function(res,num,bad,reqs)
			{
				var imgLib=this.page.imgLib;
				var cssLib=this.page.cssLib;
				var url;
				this.getItemById("num")._setText(""+num);
				url=page.genPageURL(window.imgPath+"/icon/icon_res_"+res+"64_32.png");
				if(bad)
				{
					this.getItemById("num").setColor(255,0,0,255);
				}
				else
				{
					this.getItemById("num").setColor(255,255,255,255);
				}
				this.getItemById("ResIcon").setTexURL(url);
				var reqField=this.getItemById("reqs");
				if(reqField)
					this.removeChild(reqField);
				var i,css,w,h,numTxtW,resIcon,icon,pos=[];
				w=230;
				this.setStateStyle(0,{css:imgLib.btn_green_u,mode3x3:1,w:w,h:h});
				this.setStateStyle(1,{css:imgLib.btn_green_d,mode3x3:1,w:w,h:h});
				this.setStateStyle(2,{css:imgLib.btn_green_g,mode3x3:1,w:w,h:h});
				resIcon=this.getItemById("ResIcon");
				resIcon.setPos([45,0,0]);
				this.getItemById("num").setPos([0,0,0]);
				if(reqs && reqs.length)
				{
					numTxtW=40;
					this.getItemById("num").setPos([0-reqs.length*(numTxtW+64+5)/2,0,0]);
					pos=[45-reqs.length*(numTxtW+64+5)/2,0,0];
					resIcon.setPos(pos);
					
					// DBOut("pos="+toJSON(pos))
					css={type:"icon",id:"reqs",pos:[pos[0]+resIcon.getW()+5,0,0],w:reqs.length*(numTxtW+64+5),h:this.getH(),anchor_v:1,items:[]};
					h=this.getH();
					for(i=0;i<reqs.length;i++)
					{
						css.items.push({id:"num",css:cssLib.textFineBig.createCSS([0+i*(numTxtW+64+5),0,0],reqs[i].num,numTxtW,this.getH(),0,1,2,1,reqs[i].bad?[255,0,0,255]:[255,255,255,255])});
						icon=imgLib.getIcon("icon_"+reqs[i].type+"_32");
						icon.w=icon.h=64;
						css.items.push({type:"icon",id:"icon",pos:[i*(numTxtW+64+5)+numTxtW+32,0,0],css:icon,anchor_h:1,anchor_v:1})
						w+=numTxtW+64+5;
					}
					// this.setW(w);
					this.setStateStyle(0,{css:imgLib.btn_green_u,mode3x3:1,w:w,h:h});
					this.setStateStyle(1,{css:imgLib.btn_green_d,mode3x3:1,w:w,h:h});
					this.setStateStyle(2,{css:imgLib.btn_green_g,mode3x3:1,w:w,h:h});
					this.appendNewChild(css);
				}
			}
		};

