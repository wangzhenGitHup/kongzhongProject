		//图片+文字
		this.cssLib.iconText=
		{
			cssLib:this.cssLib,
			imgLib:this.imgLib,
			page:this,
			createCSS:function(pos,pic,text,type,color,font,offsetY)//文字位置type:0右，1中，2左
			{
				var imgLib=this.imgLib;
				var cssLib=this.cssLib;
			//	var icon=imgLib.getIcon(pic,64);
				font=font?font:20;
				offsetY=offsetY?offsetY:0;
				var icon=pic;
				var iconX=icon.w/2, dt=3;
				var ah=0, txtX=icon.w+dt,txtW=90-icon.w;
				if(1==type){
					ah=1;
					txtX=iconX;
				}else if(2==type){
					ah=2;
					var _txtW=this.page.appEnv.getTextSize(text+"",font,0).w;//14,20,40
					txtX=_txtW;
					iconX=_txtW+iconX+dt;
				}else if(0==type){
					ah=0;
					var _txtW=this.page.appEnv.getTextSize(text+"",font,0).w;//14,20,40
					txtX=0;
					txtW=_txtW;
					iconX=txtW+iconX+dt;
				}
				return {
					type:"shape",id:"iconText",pos:pos,w:90,h:icon.h,anchor_h:0,anchor_v:1,items:[
						{type:"icon",id:"icon",pos:[iconX,0,0],anchor_h:1,anchor_v:1,css:icon},
					//	{type:"text",id:"text",pos:[txtX,0,0],anchor_h:ah,anchor_v:1,align_h:ah,align_v:1,w:90-icon.w,h:icon.h,text:text+"",font_size:20,color_r:0,color_g:0,color_b:0,color_a:200}
						{id:"text",css:cssLib.textFineSmall.createCSS([txtX-1,0+offsetY,0],text+"",txtW,icon.h,ah,1,ah,1,color?color:null),font_size:font}//14
					],
					lineType:type,
					resetPos:this.resetPos,_setText:this._setText
				};
			},
			create:function(box,pos,key,pic,text,type,color,font)
			{
				var item=box.appendNewChild(this.createCSS(pos,pic,text,type,color));
				if(2==type)
					item.resetPos();
				return item;
			},
			resetPos:function()//根据文字实际宽度重设控件坐标
			{
				var pos=[];
				var icon=this.getItemById("icon");
				var text=this.getItemById("text");
				var txtW=text.getTextW();
				text.getPos(pos);
				text.setPos([txtW,pos[1],0]);
				var iconW=icon.getW();
				icon.setPos([txtW+iconW/2,pos[1],0]);
			},
			_setText:function(txt)
			{
				this.getItemById("text")._setText(txt);
			}
		};

