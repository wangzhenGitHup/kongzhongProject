	this.cssLib.multiList={//可创建复杂的listbox
		createCSS:function(pos,w,h,keyid,itemW,itemH,subW,subH,arrange,total)
		{
			if(!itemW || !itemH || !subW || !subH)
			{
				DBOut("size error!!!");
				return;
			}
			var subNum=Math.floor(itemW/subW), subGap=0, mode=0;
			if(subNum<1){
				DBOut("****** cssLib.multiList size error ******");
			//	return;
			}else if(1==subNum){
				mode=1;//正常
			}else if(!arrange){//这样写条件判断语句是否有问题？
				mode=2;//纵排，子项横排子控件
				subGap=Math.floor((itemW-subW*subNum)/(subNum+1));
			}else if(1==arrange){
				mode=3;//横排，子项纵排子控件
				subGap=Math.floor((itemH-subH*subNum)/(subNum+1));
			}
			var subX=Math.floor((itemW-subW*subNum)/2), subY=Math.floor((itemH-subH*subNum)/2);
			this.mode=mode, this.itemW=itemW, this.itemH=itemH, this.subW=subW, this.subH=subH, this.total=total, this.subGap=subGap;
			this.itemNum=Math.ceil(total/subNum), this.subNum=subNum, this.subX=subX, this.subY=subY;
			var css={type:"listbox",id:"lbx",pos:pos,w:w,h:h,anchor_h:0,anchor_v:0,ui_event:1,sub_event:1,kye:keyid,
				arrange:arrange,end_gap:0,move_gap:20,fast_scroll:1,show_fx:0,show_align:0,hot_check:1,//hot_item:0,
				item_alpha_up:1.0,item_alpha_down:1.0,item_alpha_check:1.0,item_alpha_dimmed:1.0,
				item_size_up:1.0,item_size_down:1.0,item_size_check:1.0,item_size_dimmed:1.0,
				item_w:itemW,item_h:itemH,tick_event:1,onMoveTick:function(){},
				display:1,fade:1,fade_size:1,fade_alpha:0,clip:0,subNum:subNum,line_items:[],
				//
				mode:mode,itemW:itemW,itemH:itemH,subW:subW,subH:subH,subX:subX,subY:subY,total:total,curIdx:0,subGap:subGap,
				_addItem:this._addItem, update:this.update, findItem:this.findItem
			};
			return css;
		},
		create:function(box,pos,w,h,keyid,itemW,itemH,subW,subH,arrange,total)//arrange: 列表的滚动方向。0 为纵向滚动，1 为横向滚动。默认为 0 。
		{
			var hud=box.appendNewChild(this.createCSS(pos,w,h,keyid,itemW,itemH,subW,subH,arrange,total));
			return hud;
		},
		_addItem:function(idx,itemCSS)
		{
			if(1==this.mode)
			{
				var index=this.addItem(itemCSS);
				hud=this.itemAt(index);
			}
			else
			{
				var itemIdx=Math.floor(idx/this.subNum);
				var subIdx=idx%this.subNum;
				if(!this.itemAt(itemIdx))
				{
					this.addItem({type:"icon",pos:[0,0,0],w:this.itemW,h:this.itemH,anchor_h:0,anchor_v:0,ui_event:1});
				}
				var x, y;
				if(2==this.mode)//纵排，子项横排子控件
				{
					x=this.subGap*(subIdx+1)+subIdx*this.subW;//+this.subX;
					y=0;
				}
				else if(3==this.mode)//横排，子项纵排子控件
				{
					x=0;
					y=this.subGap*(subIdx+1)+subIdx*this.subH;//+this.subY;
				}
				if(itemCSS.pos && itemCSS.pos.length)
				{
					x+=itemCSS.pos[0];
					y+=itemCSS.pos[1];
				}
				else
				{
					if(itemCSS.x)
						x+=itemCSS.x;
					if(itemCSS.y)
						y+=itemCSS.y;
				}
				hud=this.itemAt(itemIdx).appendNewChild({id:"sub"+subIdx,pos:[x,y,0],css:itemCSS});
			}
			this.curIdx++;
			return hud;
		},
		update:function(listVO)
		{
		//	DBOut("lbx update, vos="+listVO+" mode:"+this.mode);
			var num=this.getItemNum();
			var cnt=listVO.length;
			var i, item, vo;
			if(1==this.mode)
			{
				for(i=0; i<num; i++)
				{
					vo=listVO[i];
					item=this.itemAt(i);
					if(item && item.update)
					{
						item.update(vo);
					}
				}
			}
			else
			{
				var itemIdx, subIdx, hud;
				for(i=0; i<cnt; i++)
				{
					vo=listVO[i];
					itemIdx=Math.floor(i/this.subNum);
					subIdx=i%this.subNum;
					hud=this.itemAt(itemIdx);
					item=hud.getItemById("sub"+subIdx);
				//	DBOut(item+" === "+item.update);
					if(item && item.update)
					{
						item.update(vo);
					}
				}
			}
		},
		findItem:function(idx)
		{
			var itemIdx, subIdx, hud, item;
			itemIdx=Math.floor(idx/this.subNum);
			subIdx=idx%this.subNum;
			hud=this.itemAt(itemIdx);
			item=hud.getItemById("sub"+subIdx);
			return {item:item,idx:itemIdx};
		}
	};

	this.cssLib.genPoints=function(pos1,pos2,minSeg,retList)
	{
		var dx,dy,r,ditx,dity,nx,ny,cx,cy,rn;
		dx=pos2[0]-pos1[0];
		dy=pos2[1]-pos1[1];
		r=Math.sqrt(dx*dx+dy*dy);
		ditx=dx/r;
		dity=dy/r;
		nx=dity;ny=-ditx;
		cx=dx/2;cy=dy/2;
		rn=Math.random()*r*0.25;
		cx+=nx*rn;cy+=ny*rn;
		retList.push([pos1[0]+cx,pos1[1]+cy,0]);
		if(r>minSeg || r<20)
		{
			this.genPoints(pos1,[pos1[0]+cx,pos1[1]+cy],minSeg,retList);
			this.genPoints([pos1[0]+cx,pos1[1]+cy],pos2,minSeg,retList);
		}
	};
