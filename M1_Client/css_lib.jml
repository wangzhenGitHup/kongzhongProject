			cssLib:this.cssLib,
			imgLib:this.imgLib,
			page:this,
			createCSS:function(text,pos,w,keyid)
			{
				var imgLib=this.imgLib;
				var cssLib=this.cssLib;
				return {
					type:"key",pos:pos,id:"BtnDebug_"+text,css:cssLib.boxSolid,w:w,h:50,key:keyid,anchor_h:1,anchor_v:1,ui_event:1,button:1,down_scale:0.95,
					color_r:0,color_g:0,color_b:0,color_a:80,
					state_up:{css:cssLib.boxSolid,mode3x3:1,w:w,h:50},audio:this.page.audioObject.genFileURL("btn_click"),
					state_down:{css:cssLib.boxSolid,mode3x3:1,color_r:200,color_g:200,color_b:100,w:w,h:50},
					state_gray:{css:cssLib.boxSolid,mode3x3:1,w:w,h:50},
					display:1,fade:1,fade_size:1,fade_alpha:0,
					items:[
						{
							type:"text",pos:[0,0,0],w:w,h:50,text:text,font_size:24,align_h:1,align_v:1,anchor_h:1,anchor_v:1,
							color_r:255,color_g:255,color_b:255,color_a:255,edge:1,edge_r:0,edge_g:0,edge_b:0,edge_a:255,
						}
					],
					setText:this.setText
				};
			},
			create:function(box,text,pos,w,key)
			{
				return box.appendNewChild(this.createCSS(text,pos,w,key));
			},
			setText:function(text)
			{
				this.firstChild().setText(text);
			}
		};

